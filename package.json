{
    "name": "tamarin",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.44.0"
    },
    "icon": "icon.png",
    "repository": {"type": "git", "url": "https://github.com/gilcu3/vscode-tamarin"},
    "publisher": "gilcu3",
     "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "tamarin",
            "aliases": ["tamarin"],
            "extensions": [".spthy"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tamarin",
            "scopeName": "source.tamarin",
            "path": "./syntaxes/tamarin.tmLanguage"
        }],
        "snippets": [
            {
              "language": "tamarin",
              "path": "./snippets/snippets.json"
            }
        ]
    }
}